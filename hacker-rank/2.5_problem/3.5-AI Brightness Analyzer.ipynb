   "source": [
    "\n",
    "brightness =list(map(int,input().split()))\n",
    "avg_brightness = sum(brightness) / len(brightness)\n",
    "\n",
    "if avg_brightness  < 85 :\n",
    "  print(\"Dark Image\")\n",
    "elif avg_brightness <= 170 :\n",
    "  print(\"Normal Image\")\n",
    "else :\n",
    "  print(\"Bright Image\")\n",
    "\n"
   ]
  }
